# -*- coding: utf-8 -*-
# vim: ft=yaml
#
# Set default values.
---
values:
  lookup:
    pkg:
      name: homebrew/cask/karabiner-elements
      manager: brew
    paths:
      confdir: '.config/karabiner'
      conffile: 'karabiner.json'
      xdg_dirname: 'karabiner'
      xdg_conffile: 'karabiner.json'
    rootgroup: {{ salt['cmd.run']("stat -f '%Sg' /dev/console") }}
  # Just here for testing
  added_in_defaults: defaults_value
  winner: defaults
...
